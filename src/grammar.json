{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "prom",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_line"
      }
    },
    "_line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "sample"
        },
        {
          "type": "SYMBOL",
          "name": "_comment_line"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "_comment_line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "help_line"
        },
        {
          "type": "SYMBOL",
          "name": "type_line"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "help_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "#"
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "HELP"
          }
        },
        {
          "type": "FIELD",
          "name": "metric_name",
          "content": {
            "type": "SYMBOL",
            "name": "_metric_name"
          }
        },
        {
          "type": "FIELD",
          "name": "metric_help",
          "content": {
            "type": "SYMBOL",
            "name": "docstring"
          }
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "type_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "#"
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "TYPE"
          }
        },
        {
          "type": "FIELD",
          "name": "metric_name",
          "content": {
            "type": "SYMBOL",
            "name": "_metric_name"
          }
        },
        {
          "type": "FIELD",
          "name": "metric_type",
          "content": {
            "type": "SYMBOL",
            "name": "_metric_type"
          }
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "#"
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "PREC",
            "value": -10,
            "content": {
              "type": "PATTERN",
              "value": "[^\\n]*"
            }
          }
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "_metric_type": {
      "type": "ALIAS",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "STRING",
            "value": "counter"
          },
          {
            "type": "STRING",
            "value": "gauge"
          },
          {
            "type": "STRING",
            "value": "histogram"
          },
          {
            "type": "STRING",
            "value": "summary"
          },
          {
            "type": "STRING",
            "value": "untyped"
          }
        ]
      },
      "named": true,
      "value": "type"
    },
    "sample": {
      "type": "SYMBOL",
      "name": "expression"
    },
    "expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "metric_name",
          "content": {
            "type": "SYMBOL",
            "name": "_metric_name"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "label_set"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "metric_value",
          "content": {
            "type": "SYMBOL",
            "name": "_metric_value"
          }
        },
        {
          "type": "FIELD",
          "name": "timestamp",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_timestamp"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "_metric_name": {
      "type": "ALIAS",
      "content": {
        "type": "PATTERN",
        "value": "[a-zA-Z_:][a-zA-Z0-9_:]*"
      },
      "named": true,
      "value": "identifier"
    },
    "label_set": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "label"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SYMBOL",
                        "name": "label"
                      }
                    ]
                  }
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "STRING",
                      "value": ","
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "label": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "label_name",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "_label_name"
            },
            "named": true,
            "value": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "label_value",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "_label_value"
            },
            "named": true,
            "value": "string"
          }
        }
      ]
    },
    "_label_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "_label_value": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "PATTERN",
          "value": "([^\"\\\\\\n]|\\\\[\"\\\\n])*"
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "_metric_value": {
      "type": "ALIAS",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_float_value"
          },
          {
            "type": "STRING",
            "value": "NaN"
          },
          {
            "type": "STRING",
            "value": "+Inf"
          },
          {
            "type": "STRING",
            "value": "-Inf"
          }
        ]
      },
      "named": true,
      "value": "number"
    },
    "_float_value": {
      "type": "PATTERN",
      "value": "[+-]?([0-9]*[.])?[0-9]+([eE][+-]?[0-9]+)?"
    },
    "_timestamp": {
      "type": "ALIAS",
      "content": {
        "type": "PATTERN",
        "value": "[0-9]+"
      },
      "named": true,
      "value": "number"
    },
    "docstring": {
      "type": "PATTERN",
      "value": "([^\\n\\\\]|\\\\[n\\\\])*"
    }
  },
  "extras": [
    {
      "type": "STRING",
      "value": " "
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}