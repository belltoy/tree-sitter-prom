{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "prom",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_line"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "STRING",
            "value": "\n"
          }
        ]
      }
    },
    "_line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "metric_line"
        },
        {
          "type": "SYMBOL",
          "name": "_comment_line"
        }
      ]
    },
    "_comment_line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "help_line"
        },
        {
          "type": "SYMBOL",
          "name": "type_line"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "help_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "#"
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "HELP"
          }
        },
        {
          "type": "SYMBOL",
          "name": "metric_name"
        },
        {
          "type": "SYMBOL",
          "name": "metric_help"
        }
      ]
    },
    "type_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "#"
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "TYPE"
          }
        },
        {
          "type": "SYMBOL",
          "name": "metric_name"
        },
        {
          "type": "SYMBOL",
          "name": "metric_type"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "#"
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "PREC",
            "value": -10,
            "content": {
              "type": "PATTERN",
              "value": "[^\\n]*"
            }
          }
        }
      ]
    },
    "metric_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "counter"
        },
        {
          "type": "STRING",
          "value": "gauge"
        },
        {
          "type": "STRING",
          "value": "histogram"
        },
        {
          "type": "STRING",
          "value": "summary"
        },
        {
          "type": "STRING",
          "value": "untyped"
        }
      ]
    },
    "metric_help": {
      "type": "PATTERN",
      "value": "[^\\n]*"
    },
    "metric_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "metric_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "label_set"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "metric_value"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "timestamp"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "metric_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z_:][a-zA-Z0-9_:]*"
    },
    "label_set": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "label"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SYMBOL",
                        "name": "label"
                      }
                    ]
                  }
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "STRING",
                      "value": ","
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "label": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "label_name"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "SYMBOL",
          "name": "label_value"
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "label_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "label_value": {
      "type": "PATTERN",
      "value": "([^\"\\\\]|\\\\.)*"
    },
    "metric_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_float_value"
        },
        {
          "type": "STRING",
          "value": "NaN"
        },
        {
          "type": "STRING",
          "value": "+Inf"
        },
        {
          "type": "STRING",
          "value": "-Inf"
        }
      ]
    },
    "_float_value": {
      "type": "PATTERN",
      "value": "[+-]?([0-9]*[.])?[0-9]+([eE][+-]?[0-9]+)?"
    },
    "timestamp": {
      "type": "PATTERN",
      "value": "[0-9]+"
    }
  },
  "extras": [
    {
      "type": "STRING",
      "value": " "
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}